<p id="notice"><%= notice %></p>

<h1>Questions</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Answer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @targets.each do |question| %>
      <tr>
        <td><%= question.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
